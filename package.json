{
  "name": "@data-provider/axios",
  "version": "2.2.0",
  "description": "REST API Data Provider origin addon using Axios",
  "keywords": [
    "data-provider",
    "addon",
    "origin",
    "axios",
    "reactive",
    "data",
    "crud",
    "cache",
    "api",
    "rest"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "homepage": "https://www.data-provider.org",
  "repository": "https://github.com/data-provider/axios",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/data-provider"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint": "eslint src test *.js",
    "lint-staged": "lint-staged",
    "mocks": "mocks-server",
    "mocks:silent": "mocks-server --log=silent",
    "build": "rollup --config",
    "stryker:run": "stryker run",
    "test": "jest --runInBand",
    "mocks-and-test": "start-server-and-test mocks tcp:3100 test",
    "test:unit": "npm run mocks-and-test",
    "test:mutation": "start-server-and-test mocks:silent tcp:3100 stryker:run"
  },
  "peerDependencies": {
    "@data-provider/core": ">=2.10.0"
  },
  "dependencies": {
    "axios": "0.21.0",
    "axios-retry": "3.1.9",
    "path-to-regexp": "6.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@data-provider/core": "2.10.0",
    "@mocks-server/admin-api-paths": "1.0.8",
    "@mocks-server/main": "1.8.7",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "16.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "10.0.0",
    "@stryker-mutator/core": "4.1.0",
    "@stryker-mutator/jest-runner": "4.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.1",
    "babel-polyfill": "6.26.0",
    "cross-fetch": "3.0.6",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.6.1",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "redux": "4.0.5",
    "rollup": "2.32.1",
    "rollup-plugin-terser": "7.0.2",
    "sinon": "9.2.1",
    "start-server-and-test": "1.11.5"
  },
  "lint-staged": {
    "*.js": "eslint",
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "10.x || 12.x || 14.x || 15.x"
  }
}
