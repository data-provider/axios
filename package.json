{
  "name": "@data-provider/axios",
  "version": "2.1.1",
  "description": "REST API Data Provider origin addon using Axios",
  "keywords": [
    "data-provider",
    "addon",
    "origin",
    "axios",
    "reactive",
    "data",
    "crud",
    "cache",
    "api",
    "rest"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "homepage": "https://www.data-provider.org",
  "repository": "https://github.com/data-provider/axios",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint": "eslint src test",
    "lint-staged": "lint-staged",
    "mocks": "mocks-server",
    "build": "rollup --config",
    "test": "jest",
    "mocks-and-test": "start-server-and-test mocks tcp:3100 jest",
    "test:ci": "npm run mocks-and-test",
    "test:coverage": "npm run test:ci",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "@data-provider/core": "2.x"
  },
  "dependencies": {
    "axios": "0.19.2",
    "axios-retry": "3.1.2",
    "path-to-regexp": "6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@data-provider/core": "2.1.2",
    "@mocks-server/admin-api-paths": "1.0.5",
    "@mocks-server/main": "1.8.3",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-json": "4.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "25.3.0",
    "babel-polyfill": "6.26.0",
    "coveralls": "3.0.9",
    "cross-fetch": "3.0.4",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "jest": "25.3.0",
    "lint-staged": "10.1.2",
    "prettier": "2.0.4",
    "redux": "4.0.5",
    "rollup": "2.3.4",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-uglify": "6.0.4",
    "sinon": "9.0.2",
    "start-server-and-test": "1.10.11"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "10.x || 12.x || 13.x"
  }
}
