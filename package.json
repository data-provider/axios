{
  "name": "@data-provider/axios",
  "version": "2.1.3",
  "description": "REST API Data Provider origin addon using Axios",
  "keywords": [
    "data-provider",
    "addon",
    "origin",
    "axios",
    "reactive",
    "data",
    "crud",
    "cache",
    "api",
    "rest"
  ],
  "author": "Javier Brea",
  "license": "Apache-2.0",
  "homepage": "https://www.data-provider.org",
  "repository": "https://github.com/data-provider/axios",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/data-provider"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "lint": "eslint src test *.js",
    "lint-staged": "lint-staged",
    "mocks": "mocks-server",
    "build": "rollup --config",
    "test": "jest",
    "mocks-and-test": "start-server-and-test mocks tcp:3100 jest",
    "test:ci": "npm run mocks-and-test",
    "test:coverage": "npm run test:ci",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "stryker:dev": "stryker run stryker.conf.local.js",
    "stryker": "stryker run",
    "mocks-and-stryker": "start-server-and-test mocks tcp:3100 stryker"
  },
  "peerDependencies": {
    "@data-provider/core": "2.x"
  },
  "dependencies": {
    "axios": "0.19.2",
    "axios-retry": "3.1.8",
    "path-to-regexp": "6.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@data-provider/core": "2.6.0",
    "@mocks-server/admin-api-paths": "1.0.7",
    "@mocks-server/main": "1.8.6",
    "@rollup/plugin-babel": "5.1.0",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@stryker-mutator/babel-transpiler": "3.3.1",
    "@stryker-mutator/core": "3.3.1",
    "@stryker-mutator/javascript-mutator": "3.3.1",
    "@stryker-mutator/jest-runner": "3.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.1.0",
    "babel-polyfill": "6.26.0",
    "coveralls": "3.0.9",
    "cross-fetch": "3.0.5",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "redux": "4.0.5",
    "rollup": "2.21.0",
    "rollup-plugin-terser": "6.1.0",
    "sinon": "9.0.2",
    "start-server-and-test": "1.11.2"
  },
  "lint-staged": {
    "*.js": "eslint",
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "engines": {
    "node": "12.x || 14.x"
  }
}
